<div class="startpage">
    <div class="row">
        <div class="">
            <h1 class="frontheading1">Find Your Travel Partner</h1>
        </div>
    </div>    
    <div>


        
       
        <form class="item" action="/" method="post"  id="userInput">
            <div class="row">
                <div class="col-lg-12">
                    <input class="btn btn-light btn-lg" type="text" name="newName" placeholder="Full Name" autocomplete="on" required >
                </div>

                <div class="col-lg-12">
                    <input class="btn btn-light btn-lg" type="tel"  name="newNumber" minlength="10" maxlength="10" placeholder="Phone Number" required>
                </div>
                <div class="col-lg-12">
                    <input class="btn btn-light btn-lg" type="text"   name="newPlace" id="myInput" onkeyup="searchFun()" placeholder="Going to..." title="don't leave spaces for better result" autocomplete="on" required>
                </div>

                <div class="col-lg-12">
                    <input class="btn btn-light btn-lg" type="datetime-local" min="2023-03-16T00:00" max="2030-12-31T00:00" name="newDate" placeholder="Date" required> 
                </div>
                
                <div class="col-lg-12">
                    <button class="btn btn-light btn-lg " type="submit" name="list">Search</button>
                </div>

            </div>
           
            
        </form>
    
</div>
    </section>

<div class="container1">
